        if { (eval echo configure:2142: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest; then
          ac_pipe_works=yes
        else
          echo "configure: failed program was:" >&5
          cat conftest.c >&5
        fi
        LIBS="$ac_save_LIBS"
	CFLAGS="$ac_save_CFLAGS"
      else
        echo "cannot find nm_test_func in $ac_nlist" >&5
      fi
    else
      echo "cannot find nm_test_var in $ac_nlist" >&5
    fi
  else
    echo "cannot run $ac_cv_sys_global_symbol_pipe" >&5
  fi
else
  echo "$progname: failed program was:" >&5
  cat conftest.c >&5
