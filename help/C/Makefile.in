install-manualDATA: $(manual_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(manualdir)
	@list='$(manual_DATA)'; for p in $$list; do \
	  if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(manualdir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(manualdir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(manualdir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(manualdir)/$$p; \
	  fi; fi; \
	done

uninstall-manualDATA:
	@$(NORMAL_UNINSTALL)
	list='$(manual_DATA)'; for p in $$list; do \
	  rm -f $(DESTDIR)$(manualdir)/$$p; \
	done
